<h2>
    Search Book
</h2>

<form action="/books" method="GET">
    <label>
        Name
    </label>

    <input type="text" name="name" value="<%= searchOptions.name %>">

    <button type="submit">
        Search
    </button>
</form>

<br>

<% books.forEach(book => { %> 
    <img height="150" width="100" src="<%= book.coverImagePath %>">
    <div> <%= book.title %> </div>
<% }) %> 